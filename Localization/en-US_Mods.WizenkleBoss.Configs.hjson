UIConfig: {
	DisplayName: GUI Config
	Headers.UI: Fullscreen Interfaces

	TelescopeMovementKeyPrompt: {
		Label: "[i:WizenkleBoss/ObservatoryChartItem] Screen Pan Prompt"
